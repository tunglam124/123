package test1;
public class FindN {
    public static void main(String[] args) {
        int n = 1;
        int sum = 0;

        while (sum <= 100000) {
            sum += n;
            n++;
        }

        System.out.println("Số nguyên dương n nhỏ nhất sao cho 1+2+...+n > 100000 là: " + (n - 1));
    }
}
